{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "setup": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["@flarekit/database#build"],
      "cache": false,
      "outputs": []
    },
    "preview": {
      "dependsOn": [
        "@flarekit/database#build",
        "@flarekit/database#migrate:d1:test",
        "build"
      ],
      "outputs": [],
      "persistent": true
    },
    "test:e2e": {
      "dependsOn": [],
      "outputs": []
    },
    "test:e2e:watch": {
      "dependsOn": [],
      "outputs": []
    },
    "migrate:d1:test": {
      "outputs": []
    },
    "@flarekit/database#build": {
      "cache": false,
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["@flarekit/database#build"],
      "persistent": true,
      "cache": false,
      "inputs": [],
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["@flarekit/database#build"],
      "cache": false,
      "outputs": []
    },
    "test:watch": {
      "dependsOn": ["@flarekit/database#build"],
      "cache": false,
      "outputs": [],
      "persistent": true
    }
  }
}
