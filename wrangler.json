{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "astroflare",
  "compatibility_date": "2024-09-02",
  "compatibility_flags": ["nodejs_compat"],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "cf-astro-d1",
      "database_id": "f8763464-9a96-40d8-a492-f737027a15ef",
      "migrations_table": "astro_migrations",
      "migrations_dir": "./packages/database/migrations"
    }
  ],
  "r2_buckets": [
    {
      "binding": "STORAGE",
      "bucket_name": "cf-astro-storage"
    }
  ],
  "queues": {
    "producers": [
      {
        "binding": "QUEUE",
        "queue": "astroflare-queue"
      }
    ]
  }
}
